{
  "name": "next-nest-expo-turbo-boilerplate",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@next-nest-expo-turbo-boilerplate/api --filter=@next-nest-expo-turbo-boilerplate/web",
    "dev:mobile": "turbo dev --filter=@next-nest-expo-turbo-boilerplate/api --filter=@next-nest-expo-turbo-boilerplate/mobile",
    "lint": "turbo lint",
    "clean": "chmod +x clean.sh && ./clean.sh",
    "open:emulator": "/opt/android-studio/bin/studio.sh",
    "generate:secret": "openssl rand -base64 32"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
